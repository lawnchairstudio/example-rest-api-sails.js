<div>
  <h1><%= user.name %></h1>
  <h3><%= user.username %></h3>
  <hr>
  <h3>contact: <%= user.email %></h3>
  <hr>
  <% _.each(images, function(image) { %>
    <ul>
      <li><%= image.id %></li>
      <li><%= image.path %></li>
    </ul>
  <% }) %>


  <a href="/user/edit/<%= user.id %>">Edit</a>
  <a href="/image/new/<%= user.id %>">Add Images</a>
</div>
