<h1>Users</h1>

<% _.each(users, function(user) { %>
<ul>
  <li><%= user.id %></td>
  <li><%= user.name %></td>
  <li><%= user.title %></td>
  <li><%= user.email %></td>
  <li><a href="/user/show/<%= user.id %>">SHOW</a></td>
  <li><a href="/user/edit/<%= user.id %>">EDIT</a></td>
  <li><a href="/user/delete/<%= user.id %>">DELETE</a></td>
</ul>
<% }) %>
